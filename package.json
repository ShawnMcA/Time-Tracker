{
 "name": "time-keeper",
 "version": "1.0.0",
 "description": "App to track work hours",
 "author": "Shawn McAllister",
 "main": "main.js",
 "Dependencies": {
  "electron": "^10.1.0",
  "electron-packager": "^15.1.0"
 },
 "scripts": {
  "start": "electron .",
  "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=png/TimeKeeper.png --prune=true --out=release-builds",
  "package-win": "electron-packager . electron-Time-Keeper --overwrite --asar=true --platform=win32 --arch=ia32 --icon=png/TimeKeeper.png --prune=true --out=release-builds --version-string.CompanyName=ShawnMcAllister --version-string.FileDescription=TimeKeeper --version-string.ProductName=\"Time Keeper\"",
  "package-linux": "electron-packager . electron-Time-Keeper --overwrite --asar=true --platform=linux --arch=x64 --icon=png/TimeKeeper.png --prune=true --out=release-builds"
 },
 "devDependencies": {
  "electron": "^10.1.0",
  "electron-packager": "^15.1.0"
 }, 
 "build": {
   "win": {
     "icon": "png/TimeKeeper.png"
   }
 }
}
